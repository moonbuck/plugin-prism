{
    "version": "2.0.0",
    "title": "Moondeer's PrismJS",
    "description": "A plugin for injecting prismjs for syntax highlighting.",
    "includes": [
      "prismjs-injection.html",
      "/assets/css/prism.css"
      ],
     "fields": [
        {
          "field": "params.prismjs_theme",
          "label": "PrismJS Theme",
          "placeholder": "default"
        },
        {
          "field": "params.prismjs_font_size",
          "label": "Font Size",
          "placeholder": "0.5rem"
        }
      ]
  }